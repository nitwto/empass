{% load staticfiles %}
<!DOCTYPE html>
<html>
<head>
	<title>Faculty Recruitment Form</title>
	<!-- Latest compiled and minified CSS -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

	<!-- jQuery library -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

	<!-- Latest compiled JavaScript -->
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

	<style type="text/css">
		hr{
			border: none;
			height: 1px;
			background-color: black;
		}
		@media print {
    		@page { size: landscape; }
    		.notprint{
    			 display: none; 
    		}

    		.onlyprint{
    			display: table-row;
    		}
    		#line{
			border-left: 0px solid black;
			padding-left: 0px;
			margin-left: 0px;
			margin-top: 0px;
			}

		}
	</style>

</head>

<body>
<center> <img  src="{% static 'img/HINDI2.png' %}" align="center" style="background-color:#ffffff" height="110px" width="100%px" alt="National Institute of Technology Warangal" title="National Institute of Technology Warangal"/></center>

<div class="row" style="background-color:#003A6A; color: white;">
	<center><h2>Faculty Recruitment Form</h2></center>
</div>

<div class="row">
	<div class="col-sm-2">
			<center class="notprint" style="padding-top: 100px;">

				<a href="/recruit/viewscrutiny/{{profile.applicationID}}"><button class="btn btn-success" >&nbsp;&nbsp;View Scrutiny&nbsp;&nbsp;</button></a>
				<br>
				<br>
					<a href="/recruit/viewappl/{{profile.applicationID}}"><button class="btn btn-success" disabled>View Application</button></a>
				<br>
				<br>
					<a href="/recruit/viewannex/{{profile.applicationID}}"><button class="btn btn-success">View Annexures</button></a>
				<br>
				<br>
				<a href="/faculty_admin" type="button" class="btn btn-warning">Home</a>
				<br>
				<br>
				<a href="/faculty_admin/logout" type="button" class="btn btn-danger">Logout</a>		
			</center>
	</div>
	<div class="col-sm-9" id="line" style="border-left: 1px solid grey;padding-left: 50px;">
		<form style="margin-top:3%;" method="post" action="/">
		{% csrf_token %}
		<div class="row">
			<center>
			<h3><u> Application Form for faculty Position </u></h3>
			<h3><u>Application ID : {{college}}{{profile.applicationID}}</u></h3>
			</center>
		</div>

		<!-- ////////////////// Part1 ///////////////// -->
		<div id="part1">

			<div class="well well-lg">
			<div class="row">
			<div class="col-sm-9">
				<div class="row">
					<div class="col-sm-3"> <b>Application for the Post of</b> </div>
					<div class="col-sm-3">
						<div class="form-group" style="font-size:18px;">
							<b><u>{{profile.postApplied}}</u></b>
						</div>
					</div>
					<div class="col-sm-1"> <b>in</b> </div>
					<div class="col-sm-3">
						<div class="form-group" style="font-size:18px;">
							<b><u>{{profile.departmentApplied}}</u></b>
						</div>
					</div>
				</div>

				<div class="row">
					<div class="col-sm-3"> <b>Specialization</b> </div>
					<div class="col-sm-6">
						<div class="form-group">
							<!-- <input class="form-control" type="text" name="specialization" value="{{specialization}}" readonly> -->
							<p><u>{{specialization}}</u></p>
						</div>
					</div>
				</div>

				<div class="row">
					<div class="col-sm-3"> <b>Name in Full (In Block Letters)</b> </div>
					<div class="col-sm-3">
						<div class="form-group">
							<!-- <input class="form-control" type="text" name="Name" value="{{generalData.full_name}}" readonly> -->
							<p>{{generalData.full_name}}</p>
						</div>
					</div>
					<div class="col-sm-2"> <b>Gender</b> </div>
					<div class="col-sm-2">
						<div class="form-group">
							<!-- <input class="form-control" type="text" name="gender" value="{{generalData.gender}}" readonly> -->
							<p>{{generalData.gender}}</p>
						</div>
					</div>
				</div>
			</div>
                        <div class="col-sm-3">
                                <center>
                                        {% if profile.profilePic %}
                                        <img src="{{profile.profilePic.url}}" class="img-rounded" style="height:150px; max-width:90%;">
                                        {% else %}
                                        <img src="{% static 'img/default_profile.jpg' %}" class="img-rounded" style="height:150px; max-width:90%;">
                                        {% endif %}
                                        <br><br>
                                </center>
                       </div>
			</div>

			<div class="row">
				<div class="col-sm-3"> <b>Father's Name</b> </div>
				<div class="col-sm-3">
					<div class="form-group">
						<!-- <input class="form-control" type="text" name="fatherName" value="{{generalData.father_name}}" readonly> -->
						<p>{{generalData.father_name}}</p>
					</div>
				</div>
				<div class="col-sm-2"> <b>Occupation</b> </div>
				<div class="col-sm-2">
					<div class="form-group">
						<!-- <input class="form-control" type="text" name="fatherOccupation" value="{{generalData.father_occ}}"> -->
						<p>{{generalData.father_occ}}</p>
					</div>
				</div>
			</div>

			<div class="row">
				<div class="col-sm-3"> <b>Mother's Name</b> </div>
				<div class="col-sm-3">
					<div class="form-group">
						<!-- <input class="form-control" type="text" name="motherName" value="{{generalData.mother_name}}" readonly> -->
						<p>{{generalData.mother_name}}</p>
					</div>
				</div>
				<div class="col-sm-2"> <b>Occupation</b> </div>
				<div class="col-sm-2">
					<div class="form-group">
						<!-- <input class="form-control" type="text" name="motherOccupation" value="{{generalData.mother_occ}}"> -->
						<p>{{generalData.mother_occ}}</p>
					</div>
				</div>
			</div>

			<div class="row">
				<div class="col-sm-3"> <b>Nationality</b> </div>
				<div class="col-sm-3">
					<div class="form-group">
						<!-- <input class="form-control" type="text" name="nationality" value="{{generalData.nation}}" readonly> -->
						<p>{{generalData.nation}}</p>
					</div>
				</div>
				<div class="col-sm-2"> <b>Place of Birth</b> </div>
				<div class="col-sm-2">
					<div class="form-group">
						<!-- <input class="form-control" type="text" name="birthplace" value="{{generalData.pob}}" readonly> -->
						<p>{{generalData.pob}}</p>
					</div>
				</div>
			</div>

			<div class="row">
				<div class="col-sm-3"> <b>Date of Birth</b> </div>
				<div class="col-sm-3">
					<div class="form-group">
						<!-- <input class="form-control" type="text" name="dateOfBirth" value="{{generalData.dob}}" readonly> -->
						<p>{{generalData.dob}}</p>
					</div>
				</div>
				<div class="col-sm-1"> <b>Age</b> </div>
				<div class="col-sm-2">
					<div class="form-group">
						<!-- <input class="form-control" type="text" name="age" value="{{generalData.age}}" readonly> -->
						<p>{{generalData.age}}</p>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-3"><b>Aadhaar Number (optional)</b></div>
				<div class="col-sm-3">
					<!-- <input class="form-control" type="text" name="aadhaar" value="{{generalData.aadhaarNo}}" maxlength="12"> -->
					<p>{% if generalData.aadhaarNo %} {{generalData.aadhaarNo}} {% else %} NA {% endif %}</p>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-3"> <b>Contact</b> </div>
				<div class="col-sm-3">
					<div class="form-group" style="font-size:15px;">
						<b><u>{{profile.contact}}</u></b>
					</div>
				</div>
				<div class="col-sm-1"> <b>Email  </b> </div>
				<div class="col-sm-3">
					<div class="form-group" style="font-size:15px;">
						<b><u>{{request.user.email}}</u></b>
					</div>
				</div>
			</div>

			<div class="row">
				<div class="col-sm-3"> <b>Address of Correspondence</b> </div>
				<div class="col-sm-8">
					<div class="form-group">
						<!-- <input class="form-control" type="text" name="corresAddress" value="{{generalData.correspond_addr}}" readonly>
					-->
					<p>{{generalData.correspond_addr}}</p>
					</div>
				</div>
			</div>

			<div class="row">
				<div class="col-sm-3"> <b>Permanent Address</b> </div>
				<div class="col-sm-8">
					<div class="form-group">
						<!-- <input class="form-control" type="text" name="permanentAddress" value="{{generalData.permanent_addr}}" readonly>
					-->
					<p>{{generalData.permanent_addr}}</p>
					</div>
				</div>
			</div>

			<div class="row">
				<div class="col-sm-3"> <b>Category</b> </div>
				<div class="col-sm-6">
					<p>{{generalData.category}}</p>
				</div>
			</div>

			<div class="row">
				<div class="col-sm-3"> <b> PWD  </b> </div>
				<div class="col-sm-3">
					<p>{{generalData.pwd}}</p>
				</div>
				</div>
			</div>
			<hr>
			<center><h4><u>Payment Details</u></h4></center>
			<table id="datatable" class="display table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr style="background-color: #e7efe3;">
                <th>Bank Name</th>
                <th>Transaction ID</th>
                <th>Account No</th>
                <th>Payment Date</th>
                <th>Payment Type</th>
                <th>Amount</th>
                <th class="notprint">Receipt</th>
            </tr>
        </thead>
 
        <tbody>
			<tr>
				<td>{{paymentDetails.bankName}}</td>
				<td>{{paymentDetails.TransID}}</td>
				<td>{{paymentDetails.accountNum}}</td>
				<td>{{paymentDetails.payDate}}</td>
				<td>{{paymentDetails.payType}}</td>
				<td>{{paymentDetails.amount}}</td>
				<td class="notprint">
				{% if paymentDetails.receipt %}<a target="_blank" href="{{paymentDetails.receipt.url}}"><button class="btn btn-success">Receipt</button></a>
				{% else %}<a target="_blank" href=""><button class="btn btn-danger" disabled>Receipt</button></a>
				{% endif %}
				</td>
			</tr>
        </tbody>
    </table>
			<hr>
			<div class="row">
				<div class="col-sm-9">
					<b><h3>Particulars of Educational Qualifications (Annexure A)</h3></b>
					
				</div>
				<div class="col-sm-3">
				{% if acad_annex_a_file %}
					<a type="button" target="_blank" class="notprintbtn btn-success" href="{{acad_annex_a_file.url}}">Annexure A</a><br>
					{% else %}
					<a type="button" target="_blank" class="notprint btn btn-danger" disabled>Annexure A</a><br>
					{% endif %}
				</div>
			</div>

			<table class="table table-bordered">
				<thead>
					<tr style="background-color:#e3e3e3;">
						<th>Degree Obtained & Specializations</th>
						<th>Degree</th>
						<th>Name of University/Institute</th>
						<th>Year of Passing</th>
						<th>% of Marks / CGPA</th>
						<th>Class/Division</th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td>Bachelor's Degree</td>
						<td>{{Bqual.degreeName}}</td>
						<td>{{Bqual.university}}</td>
						<td>{{Bqual.passingYear}}</td>
						<td>{{Bqual.marks}}</td>
						<td>{{Bqual.division}}</td>
					</tr>
					<tr>
						<td>Master's Degree</td>
						<td>{{Mqual.degreeName}}</td>
						<td>{{Mqual.university}}</td>
						<td>{{Mqual.passingYear}}</td>
						<td>{{Mqual.marks}}</td>
						<td>{{Mqual.division}}</td>
					</tr>
					<tr>
						<td>Ph.D. Degree</td>
						<td>{{Phdqual.degreeName}}</td>
						<td>{{Phdqual.university}}</td>
						<td>{{Phdqual.passingYear}}</td>
						<td>{{Phdqual.marks}}</td>
						<td>{{Phdqual.division}}</td>
					</tr>
					<tr>
						<td>Other's (If any)</td>
						<td>{{Oqual.degreeName}}</td>
						<td>{{Oqual.university}}</td>
						<td>{{Oqual.passingYear}}</td>
						<td>{{Oqual.marks}}</td>
						<td>{{Oqual.division}}</td>
					</tr>
				</tbody>
			</table>
			{% if annex_a %}
			<br>
			<h3>Annexure A data</h3>
			<table class="table table-bordered">
				<thead>
			        <tr style="background-color:#e3e3e3;">
				        <th>S.No.</th>
				        <th>Qualification</th>
				        <th>Degree</th>
				        <th>%Mark/CGPA</th>
				        <th>Div.</th>
				        <th colspan="2"><center>Date</center></th>
				        <th>University/Institution</th>
			        </tr>
			        <tr style="background-color:#e3e3e3;">
			        	<th colspan="5"></th>
			            <th>Entry</th>
			            <th>Completion</th>
			            <th></th>
			        </tr>
			    </thead>
			    <tbody id="annexure_a">
			    	{% for rec in annex_a %}
			    	<tr>
			    		<td>{{forloop.counter}}</td>
			    		<td>{{rec.qualification_a}}</td>
			    		<td>{{rec.degree_a}}</td>
			    		<td>{{rec.marks_a}}</td>
			    		<td>{{rec.div_a}}</td>
			    		<td>{{rec.entry_a}}</td>
			    		<td>{{rec.completion_a}}</td>
			    		<td>{{rec.university_a}}</td>
			    	</tr>
			    	{% endfor %}
			    </tbody>
			</table>
			{% endif %}
		</div>

		<!-- ////////////////// Part2 ///////////////// -->
		<div id="part2">

			<hr>
			<div class="row">
				<div class="col-sm-6">
					<h4 style="font-weight:900;">Details of Experience</h4>
					{% if paper1 %}
						<a type="button" target="_blank" class="notprint btn btn-success" href="{{paper1.url}}">Supporting Documents</a><br>
					{% else %}
						<a type="button" target="_blank" class="notprint btn btn-danger" disabled>Supporting Documents</a><br>
					{% endif %}
				</div>
			</div>
			<br>

			<!-- Teaching Experience Code -->
			<div class="row">
				<div class="col-sm-3"> <b>Teaching Experience (years)</b> </div>
				<div class="col-sm-3">
					<div class="form-group">
						<input class="form-control" type="text" name="teachingExperience" value="{{Experience.teaching_exp}}" readonly>
					</div>
				</div>
				<div class="col-sm-3"> <b>Post Ph.D. Experience (years) </b> </div>
				<div class="col-sm-2">
					<div class="form-group">
						<input class="form-control" type="text" name="phdExperience" value="{{Experience.postPhd_exp}}" readonly>
					</div>
				</div>
			</div>

			<table class="table table-bordered">
				<thead>
				  <tr style="background-color:#e3e3e3;">
					<th>S.No.</th>
					<th>Name of Organization</th>
					<th>Designation</th>
					<th>Date of Joining</th>
					<th>Date of Leaving</th>
					<th>Last pay Band and Grade Pay</th>
					<th>Service (Regular/Contract)</th>
				  </tr>
				</thead>
				<tbody id="teachingExpTbody">
					{% for texp in teachingData %}
					<tr>
						<td>{{forloop.counter}}.</td>
						<td>{{texp.organization}}</td>
						<td>{{texp.designation}}</td>
						<td>{{texp.joiningDate}}</td>
						<td>{{texp.leavingDate}}</td>
						<td>{{texp.pay}}</td>
						<td>{{texp.service}}</td>
					</tr>
					{% endfor %}
				</tbody>
			</table>
			<br>

			<input type="text" name="teachingExpData" id="teachingExp" hidden>

			<!-- Research Experience Code -->

			<div class="row">
				<div class="col-sm-3"> <b>2. Research Experience (years)</b> </div>
				<div class="col-sm-3">
					<div class="form-group">
						<input class="form-control" type="text" name="researchExperience" value="{{Experience.research_exp}}" readonly>
					</div>
				</div>
			</div>

			<table class="table table-bordered">
				<thead>
				  <tr style="background-color:#e3e3e3;">
					<th>S.No.</th>
					<th>Name of Organization where employed</th>
					<th>Designation</th>
					<th>Date of Joining</th>
					<th>Date of Leaving</th>
					<th>Last pay Band and Grade Pay</th>
					<th>Service (Regular/Contract)</th>
				  </tr>
				</thead>
				<tbody id="researchExpTbody">
					{% for rexp in researchData %}
					<tr>
						<td>{{forloop.counter}}.</td>
						<td>{{rexp.organization}}</td>
						<td>{{rexp.designation}}</td>
						<td>{{rexp.joiningDate}}</td>
						<td>{{rexp.leavingDate}}</td>
						<td>{{rexp.pay}}</td>
						<td>{{rexp.service}}</td>
					</tr>
					{% endfor %}
				</tbody>
			</table>
			<br>

			<input type="text" name="researchExpData" id="researchExp" hidden>

			<!-- Industrial Experience Code -->

			<div class="row">
				<div class="col-sm-3"> <b>3. Industrial Experience (years)</b> </div>
				<div class="col-sm-3">
					<div class="form-group">
						<input class="form-control" type="text" name="industryExperience" value="{{Experience.industrial_exp}}" readonly>
					</div>
				</div>
			</div>

			<table class="table table-bordered">
				<thead>
				  <tr style="background-color:#e3e3e3;">
					<th>S.No.</th>
					<th>Name of Industry</th>
					<th>Designation</th>
					<th>Date of Joining</th>
					<th>Date of Leaving</th>
					<th>Salary Drawn</th>
					<th>Service (Regular/Contract)</th>
				  </tr>
				</thead>
				<tbody id="industryExpTbody">
					{% for iexp in industryData %}
					<tr>
						<td>{{for.counter}}</td>
						<td>{{iexp.organization}}</td>
						<td>{{iexp.designation}}</td>
						<td>{{iexp.joiningDate}}</td>
						<td>{{iexp.leavingDate}}</td>
						<td>{{iexp.pay}}</td>
						<td>{{iexp.service}}</td>
					</tr>
					{% endfor %}
				</tbody>
			</table>
			<br>

			<input type="text" name="industryExpData" id="industryExp" hidden>

		</div>
		<br>
		<br>
		<br>
		<br>
		<br>
		<br>
		<div id="part6">

					<div id="part6">
			<div class="well well-lg">
				<div class="row">
					<div class="col-sm-6" style="font-size:18px;"> 
						<b>(i) External Sponsored R&D Projects:</b><br><br> 
					</div>
					<div class="col-sm-2" style="float:right;">
						<input class="form-control credits" type="text" name="credits_1" value = {% if total_e  %}"{{total_e}}" {% else %} "0" {% endif %} readonly>
					</div>
				</div>

				<div class="row">
					<div class="col-sm-6"> 
						<b>Completed or Ongoing/ Patent granted</b><br><br> 
					</div>
				</div>


				<div class="row">
					<div class="col-sm-3"> 
						Total Sponsored Projects 
					</div>
					<div class="col-sm-2">
						<div class="form-group">
							<input class="form-control" type="text" name="total_projects" value="{{external_sponsored_rnd.total_projects}}" readonly>
						</div>
					</div>
